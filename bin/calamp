export PRODUCTION_IP="0.0.0.0"
export CALAMP_PORT=20500
export CALAMP_ROTATING_LOG_FILE="/tmp/calamp"
export CALAMP_LOG_FILE="/tmp/calamp.log"
export CALAMP_LOG_RAW=false
